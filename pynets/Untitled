for i in `cat /work/04171/dpisner/data/ABM/network_analysis/subjects_list.txt`; do
  j=`echo $i | cut -f 1 -d . | cut -f8 -d /`;
  if [[ $j != 9* ]]; then
      #echo $i;
      echo $j
      python3 /work/04171/dpisner/stampede2/PyNets/pynets/pynets_run.py -i "$i" -ID "$j" -a coords_power_2011 -model sps -m /work/04171/dpisner/data/ABM/ABM_nets/CCN_mask.nii.gz -min_thr 0.80 -max_thr 0.90 -mt -ns 4 &
  fi
done
