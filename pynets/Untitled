for i in `cat /work/04171/dpisner/data/ABM/network_analysis/subjects_list.txt`; do
  j=`echo $i | cut -f 1 -d . | cut -f8 -d /`;
  if [[ $j != 9* ]]; then
      #echo $i;
      echo $j
      fsl_sub -T 60 -q normal -A Machine-learning-app -N CCN -s omp_pe,20 python3 /work/04171/dpisner/stampede2/PyNets/pynets/pynets_run.py -i "$i" -ID "$j" -ua /work/04171/dpisner/atlases/craddock_1000.nii.gz -model sps -m /work/04171/dpisner/data/ABM/ABM_nets/CCN_mask.nii.gz -min_thr 0.75 -max_thr 0.85 -mt -parc
  fi
done


