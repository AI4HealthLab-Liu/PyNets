for i in `cat /work/04171/dpisner/data/ABM/network_analysis/subjects_list.txt`; do
  j=`echo $i | cut -f 1 -d . | cut -f8 -d /`;
  if [[ $j != 9* ]]; then
      echo $j
      fsl_sub -T 800 -q normal -A Machine-learning-app -N CON -s omp_pe,20 python3 /work/04171/dpisner/stampede2/PyNets/pynets/pynets_run.py -i "$i" -ID "$j" -ua /work/04171/dpisner/data/ABM/network_analysis/"$j"/"$j"_CON_parcellation_500.nii.gz -model sps -min_thr 0.80 -max_thr 0.90 -mt -plt -pm '20,10'
  fi
done
echo "DONE"

