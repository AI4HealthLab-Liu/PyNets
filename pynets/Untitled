  fi
done
echo "DONE"

for i in `cat /work/04171/dpisner/data/ABM/network_analysis/subjects_list.txt`; do
  j=`echo $i | cut -f 1 -d . | cut -f8 -d /`;
  if [[ $j != 9* ]]; then
      until [ $(squeue -u `whoami` | wc -l) -lt 30 ]; do
          sleep 10
      done
      echo $j
      fsl_sub -T 800 -q normal -A Machine-learning-app -N CCN -s omp_pe,20 python3 /work/04171/dpisner/stampede2/PyNets/pynets/pynets_run.py -i "$i" -ID "$j" -ua /work/04171/dpisner/data/ABM/network_analysis/"$j"/"$j"_CCN_parcellation.nii.gz -model sps -min_thr 0.85 -max_thr 0.95 -mt -plt -parc
  fi
done
echo "DONE"


for i in `cat /work/04171/dpisner/data/ABM/network_analysis/subjects_list.txt`; do
  j=`echo $i | cut -f 1 -d . | cut -f8 -d /`;
  if [[ $j != 9* ]]; then
      echo $j
      fsl_sub -T 200 -q normal -A Machine-learning-app -N triple_net -s omp_pe,20 python3 /work/04171/dpisner/stampede2/PyNets/pynets/pynets_run.py -i "$i" -ID "$j" -ua /work/04171/dpisner/data/ABM/network_analysis/"$j"/"$j"_triple_net_sig_parcellation.nii.gz -model sps -min_thr 0.80 -max_thr 0.90 -mt -plt -parc
  fi
done
echo "DONE"

